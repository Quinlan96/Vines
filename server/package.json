{
	"name": "server",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "nodemon --ignore src/public/static --exec 'babel-node -r ./node_modules/dotenv/config ./src/bin/www.js'",
		"format": "prettier \"./**/*.js\" --write",
		"build": "babel src/ -d dist/",
		"start": "NODE_ENV=production node -r dotenv/config dist/bin/www.js"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"csv-parser": "^2.3.2",
		"debug": "~2.6.9",
		"dotenv": "^8.2.0",
		"express": "~4.16.1",
		"http-errors": "~1.6.3",
		"mongoose": "^5.9.1",
		"mongoose-fuzzy-searching": "^1.1.8",
		"morgan": "~1.9.1",
		"node-fetch": "^2.6.0",
		"youtube-dl": "^3.0.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.8.4",
		"@babel/node": "^7.8.4",
		"@babel/preset-env": "^7.8.4",
		"nodemon": "^2.0.2",
		"prettier": "^1.19.1"
	},
	"babel": {
		"presets": [
			"@babel/preset-env"
		],
		"ignore": [
			"../public/static"
		]
	}
}
